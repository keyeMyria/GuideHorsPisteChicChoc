    "flow-bin": "^0.71.0",
    "jest": "22.4.3",
    "jest-serializer-enzyme": "^1.0.0",
    "prettier-eslint": "^8.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/enzymeSetup.js"
  }
