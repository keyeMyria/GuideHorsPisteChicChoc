    "@turf/bezier-spline": "^6.0.2",
    "@turf/destination": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/turf": "^5.1.6",
    "bugsnag-react-native": "^2.9.5",
    "moment": "^2.22.2",
    "native-base": "^2.6.1",
    "npm-check": "^5.7.1",
    "react": "16.3.1",
    "react-dom": "^16.4.0",
    "react-native": "^0.55.4",
    "react-native-datepicker": "^1.7.2",
    "react-native-permissions": "^1.1.1",
